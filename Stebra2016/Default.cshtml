<div class="container-fluid dividers1 map">
    <div id="kontakta" class="container bg-grey">
        <h2>Kontaka oss</h2>
        <br>

        <div class="row">
            <div class="col-lg-6 col-lg-push-3 col-sm-9 col-xs-12 form-group">

                <form action="~/Default.cshtml" href="#kontakta">
                    <span class="glyphicon glyphicon-user pull-left contact-glyphicon"></span>
                    <input class="form-control" id="name" name="name" placeholder="Namn" type="text" required="">



                    <span class="glyphicon glyphicon-envelope pull-left contact-glyphicon"></span>
                    <input class="form-control" id="email" name="email" placeholder="Email" type="email" required="">

                    <span class="glyphicon glyphicon-comment pull-left contact-glyphicon"></span>
                    <textarea class="form-control" id="comments" name="comments" placeholder="Meddelande / Lämna ditt mobilnummer om du vill bli uppringd" rows="8"></textarea><br>

                    <button id="submitMyMessage" class="btn btn-default pull-right" type="submit" name="contactForm" value="xx">Skicka till info@Stebra.se</button>
                </form>

            </div>
        </div>
    </div>
</div>


