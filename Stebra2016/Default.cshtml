@{
    Layout = "~/_Layout.cshtml";
    Page.Title = "Stebra Consulting";
    List<StebraEntity> sortedEntries = AzureManager.sort();
    var t = sortedEntries[0].Timestamp;
    sortedEntries[0].sweDate = t.Day + "-" + t.Month + "-" + t.Year;
    var result = Request["contactForm"];
    var name = Request["name"];
    var email = Request["email"];
    var comments = Request["comments"];

    if (result != null)
    {
        <div id="isPostBack" value="true">
            @Mail.Send(name, email, comments);
        </div>

    }
}


